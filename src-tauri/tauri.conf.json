{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sheetsliderdemo",
  "version": "1.0.0",
  "identifier": "com.nakanishi.sheetsliderdemo",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "sheetsliderdemo",
        "width": 2404,
        "height": 1080,
		"resizable": true,
		"fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
